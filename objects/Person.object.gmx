{
show_debug_message("--------------------calling ASTAR------");
    //the use of these vars is an ugly hack to compensate for the tile grid
    //not starting at 0,0

    tilepath=Astar(x div 31,y div 31,target_x div 31,target_y div 31);
    show_debug_message("Astar call complete");
    path_length=array_length_1d(tilepath);
    show_debug_message(path_length);
    index_into_path=0;
    if(tilepath==noone)
