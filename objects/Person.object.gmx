            <string>/// move to target if not on target and path exists

if(tilepath==noone)
    exit;
if( x-deltafy(velocity) &lt;= target_x &amp;&amp; target_x &lt;= x+deltafy(velocity) )
{
    x=target_x;
}

if(x &lt; target_x)
{
    x=x+deltafy(velocity);
    exit;
}


if( target_x &lt; x )
{
    x=x-deltafy(velocity);
    exit;
}


if( y-deltafy(velocity) &lt;= target_y &amp;&amp; target_y &lt;= y+deltafy(velocity) )
{
    y=target_y
    exit;
}

if(y &lt; target_y)
{
    y=y+deltafy(velocity);
    exit;
}


if( target_y &lt; y )
{
    y=y-deltafy(velocity);
    exit;
}


