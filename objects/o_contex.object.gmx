                //update funds to price of station/2
                ctrl.funds += ctrl.store[#store_get_id(ctrl.store, station.object_index), 4]/2; 
                var tex = instance_create(x,y,o_fadetext);
                tex.text = ctrl.store[#store_get_id(ctrl.store, station.object_index), 4]/2;
                audio_play_sound(snd_cash,1,false);
                
                matrix_clear(xx div 31, yy div 31, station.width, station.height, station);
                for(var i =0; i &lt; ds_list_size(global.built_stations);i++)
                {
                    if(global.built_stations[|i]==station)
                    {
                        ds_list_delete(global.built_stations,i);
                    }
                }
                instance_destroy();
                with(station){
                    instance_destroy();
                }
                ctrl.profileActive = false;
                exit;
            }        
        }
    }
}
