            <string>///check if valid to build, and build if clicked
var isValid;
isValid = true;
image_blend = c_white;

//If we're not already placed
if(being_built &amp;&amp; realmx() &gt; display_get_gui_width()/6){
    //are we valid?
    if(!matrix_is_empty(x div 31,y div 31,width,height) || !wallmatrix_is_empty(x div 31,y div 31,width,height)){
        isValid = false;
        image_blend = c_red;
    }
    if(y &gt; 576){
        isValid = false;
        image_blend = c_red;
    }
    //move me
    x = (mouse_x div 31 * 31);
    y = (mouse_y div 31 * 31);
    //moving the bed
    
    if(move_bed){
        x = (mouse_x div 31 * 31);
        y = (mouse_y div 31 * 31);
        if(mouse_check_button_pressed(mb_right)){
            if(isValid){
                being_built = false;
                move_bed = false;
                ctrl.isClicked = false;
                matrix_reserve(x div 31, y div 31, width,height,id);//place me.
            }
        }
    }
    else{
        //We are pressed
        if(mouse_check_button_pressed(mb_left)){
            if(isValid){//and valid
                if(ctrl.funds &gt;= price){//and affordable
                    o_ui_night.I = noone;
                    o_ui_night.cancel_build = false;
                    o_ui_night.hover = true;
                    being_built = false;
                    ctrl.funds -= price;
                    var tex = instance_create(x,y,o_fadetext);
                    tex.text = price;
                    audio_play_sound(snd_cash,1,false);
                    ds_list_add(global.built_stations,id);
                    matrix_reserve(x div 31, y div 31, width,height,id);//place me.
                }
            }
        }
        //For debug, shouldn't need anymore unless cancel button doesn't work
        /*if(mouse_check_button_pressed(mb_right)){
            instance_destroy();
        }*/
    }
}
